#!/usr/bin/env python

""" CIDR tools
"""

import sys
import netaddr

def addr_in_cidr():
    """.
    """
    network = netaddr.IPNetwork(sys.argv[2])
    if netaddr.IPAddress(sys.argv[1]) in network:
        print "yes"

    for addr in network:
        print addr


def main():
    """Entry"""
    cidr = sys.argv[1]
    network = netaddr.IPNetwork(cidr)
    for ipaddr in network:
        print ipaddr

if __name__ == '__main__':
    main()

