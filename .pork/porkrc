set FORMAT_CHAT_UNIGNORE %W$N%x has %Gunignored%x $D in %c$R
set FORMAT_FILE_CANCEL_LOCAL [$T] $U has canceled file transfer $I with $u for $N. $s bytes were sent
set FORMAT_FILE_CANCEL_REMOTE [$T] $u has canceled file transfer $I with $U for $N. $s bytes were sent
set FORMAT_FILE_LOST [$T] File transfer $I with $u for $N has been lost. $s bytes were sent
set FORMAT_FILE_RECV_ACCEPT [$T] $U has accepted $N from $u. Beginning transfer
set FORMAT_FILE_RECV_ASK [$T] $u [$F:$p] offered $N ($S bytes) to $U. Type /file get $I to accept it
set FORMAT_FILE_RECV_COMPLETE [$T] Transfer $I has completed successfully: $N ($s bytes) was received by $U from $u in $t seconds ($R KB/s)
set FORMAT_FILE_RECV_RESUME [$T] $U has accepted $N from $u, resuming at byte $O
set FORMAT_FILE_SEND_ACCEPT [$T] $u [$F:$p] has accepted $N from $U. Beginning transfer
set FORMAT_FILE_SEND_ASK [$T] Sent request to send $N ($S bytes) from $u to $U, waiting for reply
set FORMAT_FILE_SEND_COMPLETE [$T] Transfer $I has completed successfully: $N ($s bytes) was sent by $U to $u in $T seconds ($R KB/s)
set FORMAT_FILE_SEND_RESUME [$T] $u [$F:$p] has accepted $N from $U, resuming at byte $O
set FORMAT_IM_RECV [$T] %m<%x$N%m>%x $M
